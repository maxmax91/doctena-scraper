{
    "manifest_version": 3,
    "name": "Web Scraper Extension",
    "version": "1.0",
    "description": "An extension to scrape web pages.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    }
}